def calibrateCameraROExtended(
    objectPoints,
    imagePoints,
    imageSize,
    iFixedPoint,
    cameraMatrix,
    distCoeffs,
    rvecs=...,
    tvecs=...,
    newObjPoints=...,
    stdDeviationsIntrinsics=...,
    stdDeviationsExtrinsics=...,
    stdDeviationsObjPoints=...,
    perViewErrors=...,
    flags=...,
    criteria=...,
) -> Tuple[
    retval,
    cameraMatrix,
    distCoeffs,
    rvecs,
    tvecs,
    newObjPoints,
    stdDeviationsIntrinsics,
    stdDeviationsExtrinsics,
    stdDeviationsObjPoints,
    perViewErrors,
